{
	"name": "finance-manager-server",
	"version": "1.0.0",
	"description": "",
	"private": true,
	"type": "module",
	"scripts": {
		"dev-watch": "deno run --watch --allow-all server_source/index.ts ./.dev.env",
		"test": "deno test --clean --allow-write=./ --allow-ffi --allow-env --allow-read --allow-net ./tests --quiet --trace-leaks --coverage=coverage && deno coverage --include=\"./server_source\" && deno coverage --lcov --output=./coverage/lcov.info --include=\"./server_source\"",
		"run_prod": "deno run --allow-all server_source/index.ts ./.prod.env",
		"dev_watch_node": "tsc-watch -p \"./tsconfig.json\" --onSuccess \"node server_build/index.js ./.dev.env\"",
		"dev_once_node": "tsc && node server_build/index.js ./dev.env",
		"run_prod_node": "node ./server_production/index.js ./.env",
		"build_prod_node": "tsc --outDir ./server_production",
		"docs:dev_node": "vuepress dev docs",
		"docs:build_node": "vuepress build docs",
		"test_node": "tsc -p ./tests/tsconfig.json && node ./server_ut-build/tests/index.test.js",
		"test-no-type_node": "tsc --noCheck -p ./tests/tsconfig.json && node ./server_ut-build/tests/index.test.js"
	},
	"devDependencies": {
		"@faker-js/faker": "^9.4.0",
		"@types/jest": "^29.5.14",
		"@types/node": "^22.13.1",
		"@vitest/coverage-v8": "^3.0.5"
	},
	"browserslist": [
		"> 1%",
		"last 2 versions",
		"not dead",
		"not ie 11"
	],
	"dependencies": {
		"@types/compression": "^1.7.5",
		"@types/express": "^5.0.0",
		"@types/fs-extra": "^11.0.4",
		"@types/http-errors": "^2.0.4",
		"@types/jmespath": "^0.15.2",
		"@types/json-query": "^2.2.6",
		"@types/morgan": "^1.9.9",
		"@types/stack-trace": "^0.0.33",
		"argon2": "^0.41.1",
		"chalk": "^5.4.1",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.14.1",
		"compression": "^1.8.0",
		"decimal.js": "^10.5.0",
		"dotenv": "^16.4.7",
		"dotenv-expand": "^12.0.1",
		"esm": "^3.2.25",
		"express": "^5.0.1",
		"fs-extra": "^11.2.0",
		"fs-monkey": "^1.0.6",
		"helmet": "^8.0.0",
