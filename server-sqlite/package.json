		"test-quiet": "deno test --clean --allow-write=./ --allow-ffi --allow-env --allow-read --allow-net ./tests --quiet --coverage=coverage && deno coverage --include=\"./server_source\" && deno coverage --lcov --output=./coverage/lcov.info --include=\"./server_source\"",
		"run_prod": "deno run --allow-all server_source/index.ts ./.prod.env",
		"check": "deno check ./server_source/index.ts"
	},
